<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="plone.app.event">
<body>

<metal:content-core fill-slot="content-core">
  <metal:content-core define-macro="content-core"
                        tal:define="items view/items">
                  
    <tal:repeat repeat="item items">
    
     <h1 tal:content="item/Title"/>
     <tal:obj define="obj item/getObject">
        <p tal:replace="structure obj/text/output|nothing">Text</p>
     </tal:obj>
     <emph tal:content="item/Creator"/>
     <emph tal:content="item/created"/>
     <tal:tags repeat="tag item/Subject">
       <emph tal:content="tag"/>
     </tal:tags>

    </tal:repeat>

  </metal:content-core>
</metal:content-core>

</body>
</html>
